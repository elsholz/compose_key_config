#es custom Compose sequences for Unicode characters

# Import default rules from the system Compose file:
 include "/usr/share/X11/locale/en_US.UTF-8/Compose"

# To put some stuff onto compose key strokes:
# <Multi_key> <i> <e> : "∈" U2208 # Compose - >
# <Multi_key> <colon> <parenright> : "☺" U263A   # Compose : )
# <Multi_key> <i> <n> <e> : "∉" U2209 # Compose h n k 

<Multi_key> <p> <i> : "π"
<Multi_key> <r> <a> <t> : "ℚ"
<Multi_key> <+> <-> : "±"
<Multi_key> <c> <r> <s> : "×"
<Multi_key> <i> <m> <p> : "→"
<Multi_key> <o> <r> : "∨"
<Multi_key> <e> <q> <v> : "↔"
<Multi_key> <b> <e> <t> : "β"
<Multi_key> <a> <n> <d> : "∧"
<Multi_key> <s> <p> <r> : "⊇"
<Multi_key> <t> <s> <p> <r> : "⊃"
<Multi_key> <a> <l> : "α"
<Multi_key> <e> <s> : "∅"
<Multi_key> <f> <m> : "⊥"
<Multi_key> <i> <n> <t> : "∩"
<Multi_key> <i> <n> <e> : "∉"
<Multi_key> <l> <a> <m> : "λ"
<Multi_key> <r> <e> <a> <l> : "ℝ"
<Multi_key> <t> <s> <u> <b> : "⊂"
<Multi_key> <c> <n> : "ℂ"
<Multi_key> <v> <m> : "⊤"
<Multi_key> <s> <u> <b> : "⊆"
<Multi_key> <i> <e> : "∈"
<Multi_key> <f> <a> : "∀"
<Multi_key> <n> <n> : "ℕ"
<Multi_key> <u> <n> : "∪"
<Multi_key> <n> <o> <t> : "¬"
<Multi_key> <s> <q> <r> <t> : "√"
<Multi_key> <g> <a> <m> : "γ"
<Multi_key> <w> <n> : "ℤ"
<Multi_key> <e> <x> <q> : "∃"
<Multi_key> <^> <|> : "↑"
<Multi_key> <|> <v> : "↓"
<Multi_key> <p> <r> <o> <p> : "φ"
<Multi_key> <o> <m> : "Ω"
<Multi_key> <c> <h> <e> <c> <k> : "✓"
<Multi_key> <x> <m> "✗"
<Multi_key> <d> <e> <l> : "Δ"
<Multi_key> <s> <i> <g> : "σ"
<Multi_key> <S> <I> <G> : "Σ"





